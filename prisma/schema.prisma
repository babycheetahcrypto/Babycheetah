datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  telegramId Int      @unique
  username   String?
  coins      Int      @default(0)
  referrals  Int      @default(0)
}