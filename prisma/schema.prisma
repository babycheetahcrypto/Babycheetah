generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

model User {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  telegramId Int @unique
  username  String
  firstName String
  lastName  String
  coins     Int @default(0)
  referrals Int @default(0)
}